<?xml version="1.0"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC 
 '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>

<weblogic-rdbms-jar>
 <weblogic-rdbms-bean>
  <ejb-name>Sell</ejb-name>
  <data-source-name>MateiJDBCTxDataSource</data-source-name>
  <table-map>
  <table-name>Sell</table-name>
  <field-map>
   <cmp-field>SellID</cmp-field>
   <dbms-column>SellID</dbms-column>
  </field-map>
  <field-map>
   <cmp-field>ClientID</cmp-field>
   <dbms-column>ClientID</dbms-column>
  </field-map>
  <field-map>
   <cmp-field>ClSeqID</cmp-field>
   <dbms-column>ClSeqID</dbms-column>
  </field-map>
  <field-map>
   <cmp-field>Quantity</cmp-field>
   <dbms-column>Quantity</dbms-column>
  </field-map>   
  <field-map>
   <cmp-field>Price</cmp-field>
   <dbms-column>Price</dbms-column>
  </field-map>   
  <field-map>
   <cmp-field>ClTime</cmp-field>
   <dbms-column>ClTime</dbms-column>
  </field-map>   
  </table-map>
 </weblogic-rdbms-bean>
</weblogic-rdbms-jar>
