/*BEGIN BLOG STYLE LISTS*/
ul.blog_list { 
margin:4px 4px 0px 18px;
padding:0;
list-style-image:url(/i/bullet.gif);
}

ul.blog_list li { 
margin:0;
padding:0;
line-height: 1.8em;
border-bottom: 1px solid #eee;
padding-left: 2px
}
/*END BLOG STYLE LISTS*/

/*BEGIN LIVE_FEED*/
#live_feed { font-size: 0.9em; margin-top: 10px; }

#live_feed a, #live_feed a:link, #live_feed a:active { color: #4f697d; }
#live_feed a:visited { color:#777;}
#live_feed a:hover { color:#e69; }

#live_feed .content_header {
height: 74px;
margin: 0;
padding: 0;
border-bottom: none;
background-color: transparent;
}

#live_feed.content_win {
border: none;
margin-bottom: 0;
padding: 0;
}

#live_feed ul.blog_list { margin-bottom: 14px; }
/*END LIVE_FEED*/

/*BEGIN TOPBLOG*/
#topblog {
clear: both;
border-width: 0px 1px 0px 1px;
border-style: solid;
border-color: #a0a0a0;
margin: 0;
padding: 0;
}

#topblog .article a, #topblog .article a:link, #topblog .article a:active { color:#4f697d; border-bottom:1px dotted #4F697D; }
#topblog .article a:visited { color:#777;}
#topblog .article a:hover { color:#e69; border-bottom:1px dotted #EE6699; }

#topblog .blog_list * { border:none; }

#topblog .content_header {
height:50px;
margin: 0;
padding: 0;
background-color: #FFFFFF;
border: none;
}

#topblog .hr { margin: 4px 10px 0 10px; }

#topblog .article_title, #topblog .article_author { padding: 0 10px; }
#topblog .article { padding: 0px 10px; }

#topblog .article_author { color: #777; }

#topblog .article img { display:block; margin-bottom: 4px; }

#topblog .article img.alignleft {
float:left;
margin-right:7px;
}

#topblog .article img.alignright {
float:right;
margin-left:7px;
}

#topblog #more_posts { 
font-size: 0.8em;
border-top: 1px solid #ddd; 
padding: 4px 10px 10px 10px; 
background-color: #f9f9f9;
}

#topblog #more_posts h3 { font-size: 1.1em; margin: 0; color: #333; }

#topblog #more_posts li { line-height: 1.6em; }

#topblog a#blog_bottom img { display: block; }

#topblog #more_posts a, #topblog #more_posts a:link, #topblog #more_posts a:active { color:#4f697d; }
#topblog #more_posts a:visited { color:#777; }
#topblog #more_posts a:hover { color:#e69; }
/*END TOPBLOG*/


