div#aod {
	position: relative;
	float: left;
	margin: 0px 10px 10px 0px;
	width: 208px;
	height: 220px;
	background-color:#000;
	background-position: 4px 20px;
	background-repeat: no-repeat;
}

div#aod a#aod_anchor {
	display: block;
	width: 100%;
	height: 75%;
}

div#aod_metadata {
	position: absolute;
	bottom: 0;
	width: 198px;
	background: #000;
	filter: alpha(opacity=80);
	-moz-opacity: 0.8;
	opacity: 0.8;
	padding: 5px 5px 8px 5px;
}

div#aod_metadata * { 
	font-size: 0.9em;
	font-weight: normal;
	margin: 0;
}

div#aod_metadata .album_title * {color:#fff;}

div#aod_metadata .album_artist * {color:#aaa;}

