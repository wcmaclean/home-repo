body {
background-color:#fff; /*#EDF1F0*/
margin: 0px;
padding: 0px;
font-family: "trebuchet ms",arial;
font-size: 14px;
}

a,a:active,a:link,a:visited {
color: #6E6449;
text-decoration:none;
}

a:hover {
color: #6C2335;
text-decoration:none;
}

img {
border: 0;
}

img.amg_logo {
float: right;
margin: 3px 0px 0px 0px;
}

.head_wrapper {
float: left;
}

.clearer {
clear:both;
line-height:0;
}

.content_win {
border: 1px solid #A0A0A0;
margin-bottom: 0px;
padding: 0px 10px 0px 10px;
}

.article_title {
font-size: 13px;
color: #000;
font-weight: bold;
padding-bottom: 0px;
margin: 5px 0px 0px 0px;
border-bottom: 0px solid #ccc;
}

.article_title a,.article_title a:active,.article_title a:link,.article_title a:visited {
color: #444;
}

.article_author {
font-size: 13px;
color: #6C2335;
margin: 0px 0px 5px 0px;
}

.article_author a,.article_author a:active,.article_author a:link,.article_author a:visited {
color: #6C2335;
}

.article_author a:hover {
color: #6D7C8D;
}

.hr {
display: block;
clear: both;
line-height: 0px;
height: 0px;
margin: 0px 0px 0px 0px;
background-color: #ccc;
border-bottom: 1px solid #ccc;
}

.article {
margin-top: 5px;
font-size: 12px;
color: #2B2B2B;
}

.article a,.article a:active,.article a:link,.article a:visited {
color: #6C2335;
}

.article a:hover {
color: #6D7C8D;
}

.content_header{
height:20px;
margin: 0px -10px 0px -10px;
padding: 0px 0px 0px 5px;
background-color: #6D7C8D;
border-bottom: 1px solid #A0A0A0;
}

img.archive {
float:right;
}

.bottom_tab {
margin-top: 10px;
position:relative;
clear:both;
width: 100%;
left: 10px;
text-align: right;
}

.list_item {
font-size: 11px;
}

.album_cover {
float: left;
margin-bottom: 0px;
min-height: 70px;
}

.album_title {
word-wrap: break-word;
color: #6C2335;
margin: 0px 0px 0px 85px;
}

.album_title a,.album_title a:active,.album_title a:link,.album_title a:visited,.album_title a:hover {
color: #6C2335;
font-weight: normal;
text-decoration: none;
}

.album_artist {
word-wrap: break-word;
font-size: 11px;
color: #666;
margin: 0px 0px 0px 85px;
}

.album_artist a,.album_artist a:active,.album_artist a:link,.album_artist a:visited,.album_artist a:hover {
color: #666;
text-decoration: none;
}

.album_rating {
margin: 0px 0px 0px 85px;
}

.buttons {
margin: 0px 0px 0px 85px;
text-align: right;
}

.buttons img{
margin-left: 4px;
}

.divider {
clear: both;
line-height: 0px;
font-size: 0px;
height: 10px;
margin: 0px 0px 10px 0px;
border-bottom: 1px solid #ccc;
}

.video_title {
margin-top: 10px;
}

.video_title a,.video_title a:active,.video_title a:link,.video_title a:visited,.video_title a:hover {
color: #6C2335;
font-weight: normal;
text-decoration: none;
}

.video_artist a,.video_artist a:active,.video_artist a:link,.video_artist a:visited,.video_artist a:hover {
color: #666;
text-decoration: none;
}

