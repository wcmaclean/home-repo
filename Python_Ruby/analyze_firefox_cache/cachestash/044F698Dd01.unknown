}

div#aod_metadata .buttons img{ margin-left: 4px;}

div#aod_new {
	position: relative;
	float: left;
	margin: 0px 18px 10px 0px;
	width: 200px;
	height: 222px;
	background-color:#fff;
	background-position: 0px 22px;
	background-repeat: no-repeat;
}

div#aod_new a#aod_anchor_new {
	display: block;
	width: 100%;
	height: 198px;
}

div#bboat_ad {
