<script type="text/javascript">
var rDays=0; var rYears=0; var rYDays=0; var rHours=15; var rMinutes=6; var rSeconds=0; var rMSeconds=54360445;
function countDown() {
	if ( rSeconds < 0 ) { rSeconds = 59; rMinutes--; }
	if ( rMinutes < 0 ) { rMinutes = 59; rHours--; }
	if ( rHours < 0 ) window.location.reload();
	if ( rDays != 0 && document.getElementById("rDays") != null ) document.getElementById("rDays").innerHTML = rDays == 1 ? rDays + ' day ' : rDays + ' days ';	// total days only
	else {	// years and days
		if ( rYears != 0  && document.getElementById("rYears") != null ) document.getElementById("rYears").innerHTML = rYears == 1 ? rYears + ' year ' : rYears + ' years ';
		if ( rYDays != 0  && document.getElementById("rYDays") != null ) document.getElementById("rYDays").innerHTML = rYDays == 1 ? rYDays + ' day ' : rYDays + ' days ';
	}
	if ( document.getElementById("rHours") != null ) {
		if ( rHours != 0 ) document.getElementById("rHours").innerHTML = rHours == 1 ? rHours + ' hr ': rHours + ' hr ';
		if ( document.getElementById("rMinutes") != null ) {
			if ( rMinutes != 0 ) {document.getElementById("rMinutes").innerHTML = rMinutes == 1 ? rMinutes + ' min ' : rMinutes + ' min ';  } else {document.getElementById("rMinutes").innerHTML = ' '};
			if ( document.getElementById("rSeconds") != null ) {
				document.getElementById("rSeconds").innerHTML = rSeconds == 1 ? rSeconds + ' sec' : rSeconds + ' sec ';
			}
		} else document.getElementById("rHours").innerHTML = rHours == 1 ? rHours + ' hr' : rHours + ' hr';
	}
	rSeconds--;
	setTimeout('countDown()',1000);
}
function nonDOM() {
	if ( rDays == 0 && rYDays == 0 && rHours == 0 && rMinutes == 0 ) window.location.reload();
	else {
		if ( document.rDays != null ) document.write(rDays + ' days, ');
		else if ( document.rYears != null ) document.write(rYears + ' years, ' + rYDays + ' days, ');
		if ( document.rHours != null ) document.write(rHours + ' hr, ');
		if ( document.rMinutes != null ) document.write(rMinutes + ' min');
	}
}
document.getElementById ? countDown() : nonDOM();
</script>




