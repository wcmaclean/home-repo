	function getAdIframeUrl(w,h,p) {
		var dc_size = w + "x" + h;
		var dc_site = c_domain[club] + ".mlb";

		if (typeof p != "undefined") dc_tiles[dc_size] = p;
		else {
			if ( dc_tiles[dc_size] == "undefined" || dc_tiles[dc_size] == null ) dc_tiles[dc_size] = 1;
			else dc_tiles[dc_size]++;	
		}

		dc_numads++;
		var dc_page = ( section == "undefined" || section == null ) ? "empty" : section;  // section is defined in page wrapper
		var dc_keyValPairs = ( dc_keyVal != "" ) ? dc_keyVal : "";	

		var ad_url = top.location.protocol + "//ad.doubleclick.net/adi/" + dc_site + "/" + dc_page + ";" + dc_keyValPairs + ";";
		ad_url += "pos="  + dc_tiles[dc_size] + ";";
		ad_url += "sz="   + dc_size + ";";
		ad_url += "tile=" + dc_tiles[dc_size] + ";";
		ad_url += "ord="  + ran_number + "";

		return ad_url;
	}
	
	function showMWAd(position){
		var adFrame = document.getElementById("mediaWallAd");
		adFrame.src = getAdIframeUrl(300,250,position);
