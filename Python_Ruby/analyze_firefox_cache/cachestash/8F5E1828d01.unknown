/*  UNIVERSAL HEADER STYLES */
/*  remove reset fields when yui reset/fonts are available */
#news-header {font:13px/122% arial,helvetica,clean,sans-serif;*font-size:small;*font:x-small;z-index:998; margin:0; zoom:1; position:relative; background-color: #fff; margin: 0 auto; padding-bottom: 14px; width: 750px; text-align: left;}
#news-header form, #news-header fieldset, #news-header ul, #news-header ul li {margin:0; padding:0; border:0;}
/*  --/remove reset fields when yui reset/fonts available */

#news-header a {text-decoration: none;}
#news-header a:hover {text-decoration: underline;}
#news-header a:focus {-moz-outline-style: none; border: none;}
#news-header:after, #news-header .info:after { content:"."; display:block; height:0; clear:both; visibility:hidden; }

#news-header .info{background:#efefef;border-bottom:1px solid #dedede; zoom:1; font: normal 77% verdana; padding-bottom: 4px;}
::root #news-header .info{padding: 1px 0 3px;}
#news-header .info .open {display:block;}

ul#navlinks {float: left; padding: 4px 5px 0; font-family: arial;}
ul#navlinks li, #news-header ul.login li {display:inline;margin-right:7px;padding-right:7px;list-style:none; border-right:1px solid #ccc; zoom:1; }
ul#navlinks li {float: left; vertical-align: middle;}
#news-header .info .login, #news-header .info a {color: #666;}
ul#navlinks li.more {position: relative;}
ul#navlinks li.more a {padding-right:17px;background:url(http://l.yimg.com/us.yimg.com/i/us/nws/common/news_header2.gif) right -4px no-repeat;}

ul#navlinks li ul {display:none; z-index: 999;padding:0; position:absolute;top:1.655em; left:-7px; width: 8.5em; list-style-type:none;border-bottom:2px solid #ccc;border-right:2px solid #ccc;border-top:#2px solid #efefef;background:#efefef;}
ul#navlinks li li, ul#navlinks li li.last  {float:none; text-align:left;padding:1px 7px 1px 8px;border:0; margin:0; display:block;}
ul#navlinks li li.last a {border-top:1px solid #ccc;padding-top:3px; line-height: 1.3em}
ul#navlinks li.more li a {display:block; color: #666; background:none; padding:0;}
#news-header ul li.last, #news-header ul li.welcome {border:0; padding-right:0;}

#news-header .info .login {float: right; padding: 4px 0 0;}
#news-header .info .login span {color: #000;}
#news-header .info .login strong {font-weight: bold;}

#news-header .main {border-top: 1px solid #B3B3B3;}
#news-header .logo { float:left; padding: 6px 5px 0; margin-bottom:0;}
#news-header .logo a { display:block; background:transparent url(http://l.yimg.com/us.yimg.com/i/us/nws/common/news_header2.gif) no-repeat; text-indent:-100em; overflow:hidden; height:28px; width:208px; background-position:-5px -19px;}
#news-header .logo a.verizon {height:39px; width:191px; background-position:-459px -6px;}
#news-header .logo a.att {height:37px; width:210px; background-position:-228px -14px;}
#news-header .logo a.rogers {height:37px; width:210px; background-position:-5px -62px;}
/* cobranded header */
#news-header.finance .logo, #news-header.branded .logo { position:relative; width:300px; }
#news-header.finance .logo a.verizon, #news-header.finance .logo a.att, #news-header.finance .logo a.rogers, #news-header.branded .logo a.verizon, #news-header.branded .logo a.att, #news-header.branded .logo a.rogers { margin-bottom:2px; }
#news-header.finance .cobrand, #news-header.branded .cobrand { position:absolute; bottom:-10px; left:110px; font-size:78%; color:#9c9c9c; }
#news-header.finance .cobrand a, #news-header.branded .cobrand a { display:inline; height:auto; width:auto; text-indent:none; color:#545454; line-height:1; background:none; }

#news-header .search { float:right; text-align:right; padding: 11px 5px 0; font: normal 77% verdana;}
#news-header .search legend {display:none;}
#news-header .search label { color:#666; font-weight: bold; vertical-align: middle; padding-right: 5px;}
#news-header .search input#p { width:209px; border-bottom:1px solid #ccc; padding:1px; margin:0px -2px 0 0; vertical-align:middle; font-size:103%; font-family:verdana;}
#news-header .search .ygbt { width: 9em; padding:0 0 1px; _padding: 1px 0 2px; border:1px solid #999b9a; *border:0; _margin: -1px; vertical-align: middle; overflow:hidden; cursor:pointer; cursor:hand; font:bold 100%/1.3 verdana; text-align:center; background:#ce9200 url(http://l.yimg.com/us.yimg.com/i/us/nws/common/news_header2.gif) -680px -8px repeat-x;}
#news-header .search span {*display:inline-block; border:0;*border:1px solid #999b9a; *vertical-align: middle;}
/* safari hacks for submit button */
::root #news-header .search input#p {margin-right:-1px; }
::root #news-header .search .ygbt {padding: 1px 0 2px; }
body:first-of-type #news-header .search .ygbt {padding: 1px 0 2px; }

